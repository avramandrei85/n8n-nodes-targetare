{
  "name": "n8n-nodes-targetare",
  "version": "1.0.0",
  "description": "Official community node to connect n8n to the Targetare.ro API",
  "keywords": [
    "n8n-nodes",
    "n8n-community-node",
    "targetare",
    "automation",
    "integration"
  ],
  "license": "MIT",
  "author": {
    "name": "Avram Andrei",
    "email": "aavram@makeitfuture.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avramandrei85/n8n-nodes-targetare.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && npm run copy:svg",
    "copy:svg": "cp targetare.svg dist/nodes/Targetare/targetare.svg",
    "prepublishOnly": "npm run build"
  },  
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TargetareAPI/TargetareAPI.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Targetare/Targetare.node.js"
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  }
}
